{
  "name": "react-redux-quiz-app",
  "version": "1.0.1",
  "description": "A Quiz App for Learning",
  "main": "dist/server/index.js",
  "repository": "https://github.com/bonham000/react-quiz-app",
  "scripts": {
    "lint:server": "eslint src/server",
    "lint:client": "eslint src/client",
    "lint": "npm-run-all --silent lint:* || exit 0",
    "clean": "rm -rf dist/",
    "create:dist": "mkdir -p dist/client/ && mkdir -p dist/server/",
    "create:html": "cp src/client/index.html dist/client/",
    "predev": "npm-run-all create:*",
    "dev": "cross-env NODE_ENV=development nodemon --watch src/server --exec \"babel-node src/server/index.js\"",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "prebuild": "npm-run-all lint clean create:*",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "babel src/server -d dist/server",
    "build": "npm-run-all build:*",
    "start": "./node_modules/.bin/babel-node dist/server/index.js"
  },
  "author": "Sean Smith",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-import-resolver-node": "^0.2.3",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.0",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "postcss-loader": "^0.13.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.15.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.13.0",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "express-session": "^1.14.2",
    "http": "0.0.0",
    "jsonwebtoken": "^7.1.9",
    "mongodb": "^2.2.11",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "passport-twitter": "^1.0.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "uuid-v4": "^0.1.0",
    "validator": "^6.1.0"
  }
}